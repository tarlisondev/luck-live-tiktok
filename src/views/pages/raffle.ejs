<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head'); %>

    <body>
        <%- include("../partials/header"); %>
            <section class="container">
                <h2><%= title %></h2>
                <div>
                    <% data.map((item)=> { %>
                        <div>
                            <img src="http://localhost:3333/public/imgs/<%= item.image%>" alt="<%= item.name %>">
                            <h2>
                                <%= item.title %>
                            </h2>
                            <p>
                                <%= item.description %>
                            </p>
                            <span>
                                <%= item.amount%>
                            </span>
                        </div>

                        <% }); %>
                </div>
            </section>
            <%- include("../partials/footer"); %>
    </body>
</html>